    "from github import Github\n",
    "from dotenv import load_dotenv\n",
    "\n",
    "import os\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "access_token = os.getenv('GH_ACCESS_TOKEN')\n",
    "\n",
    "g = Github(login_or_token=access_token)\n",
    "\n",
    "for repo in g.get_user().get_repos():\n",
    "    print(repo.name)"
   ]
